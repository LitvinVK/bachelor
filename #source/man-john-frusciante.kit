    <script src="js/components/register-man.js"></script>
    <script src="js/man-john-frusciante/categories-man.js"></script>
    <script type="text/javascript" src="js/scripts/script-more.js"></script>
    <script src="js/man-john-frusciante/shopping-john-frusciante.js"></script>
    <script src="js/man-john-frusciante/footer-shopping-john-frusciante.js"></script>
</body>

</html>